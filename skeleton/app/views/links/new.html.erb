<h1>New Link</h1>
<form action="<%=links_url%>" method="POST">
  <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
  <label for="title">Title: 
    <input type="text" id="title" name="link[title]" value="<%= @link.title %>">
  </label>
  <label for="url">URL:
    <input type="text" id="url" name="link[url]" value="<%= @link.url %>">
  </label>
  <%= flash[:errors] %>
  <input type="submit" value="Create New Link">
  </form>